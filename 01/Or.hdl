 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // Or Ã¨ molto simile a Nand, ha tre punti di output
    // In cui sono 1, devo cercare di invertire 0 0 con 1 1
    // E Not in entrambi funziona.
    Not(in=a, out=Nota);
    Not(in=b, out=Notb);
    Nand(a=Nota, b=Notb, out=out);
}
